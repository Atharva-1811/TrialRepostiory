<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>embelSoft</display-name>
  <welcome-file-list>
    <welcome-file>/jsp/login.jsp</welcome-file>
  </welcome-file-list>
  

    

  
  <servlet>
  	<servlet-name>BulkUploadController</servlet-name>
  	<servlet-class>com.embelSoft.controller.BulkUploadController</servlet-class>
  </servlet> 
   <servlet-mapping>
  	<servlet-name>BulkUploadController</servlet-name>
  	<url-pattern>/BulkUploadController</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>StockBulkUploadController</servlet-name>
  	<servlet-class>com.embelSoft.controller.StockBulkUploadController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>StockBulkUploadController</servlet-name>
  	<url-pattern>/StockBulkUploadController</url-pattern>
  </servlet-mapping>

   <listener>
    <listener-class>org.quartz.ee.servlet.QuartzInitializerListener</listener-class>
  </listener>
</web-app>
 