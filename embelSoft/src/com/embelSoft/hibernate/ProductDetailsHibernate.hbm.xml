<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
    
    <class name="com.embelSoft.hibernate.ProductDetailsHibernate" 
    table="product_details" >
    
    	<id name="pkProductDetailsId" type="java.lang.Long" column="pk_product_details_id">
			<generator class="increment" />
		</id>
		
	<property
        name="productName"
        type="java.lang.String"
        column="product_name"
    />
    
    <property
        name="productCost"
        type="java.lang.Double"
        column="product_cost"
    />
    
    <property
        name="description"
        type="java.lang.String"
        column="description"
    />
       <property
			name="hsnsac"
        	type="java.lang.String"
        	column="hsn_sac"
       />
    <property
        name="features"
        type="java.lang.String"
        column="features"
    />
    
    
    <property
        name="fkTaxTypeId"
        type="java.lang.Long"
        column="fk_tax_type_id"
    />
    
    <property
        name="taxType"
        type="java.lang.String"
        column="tax_type"
    />
    
    <property
        name="taxPercentage"
        type="java.lang.Double"
        column="tax_percentage"
    />
    
    <property
        name="CGST"
        type="java.lang.Double"
        column="CGST"
    />
    
    <property
        name="SGST"
        type="java.lang.Double"
        column="SGST"
    />
    
    <property
        name="lastUpdateDate"
        type="java.sql.Date"
        column="last_update_date"
    />
 
		</class>
    </hibernate-mapping>