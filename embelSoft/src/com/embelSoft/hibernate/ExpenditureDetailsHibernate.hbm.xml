<?xml version="1.0"?>
<!-- ExpenditureDetailsHibernate.hbm.xml -->
 <!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="com.embelSoft.hibernate.ExpenditureDetailsHibernate"
		table="expenditure_details" >
		<id name="pkExpenseId" type="java.lang.Long">
			<column name="pk_expense_id" />
			<generator class="increment" />
		</id>
		<property name="expenseName" type="java.lang.String">
			<column name="expense_name" />
		</property>
		<property name="lastUpdateDate" type="java.sql.Date">
			<column name="last_update_date" />
		</property>
	</class>
</hibernate-mapping>